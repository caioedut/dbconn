{
  "name": "dbconn",
  "productName": "DbConn",
  "version": "0.2.7",
  "private": true,
  "main": "main/out/main.js",
  "homepage": "./",
  "description": "Multi-database connect client",
  "author": "test",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -k \"cross-env BROWSER=none npm run next:dev\" \"npm run electron:dev\"",
    "build": " npm run build:main && next build",
    "start": "cross-env npm run electron",
    "build:main": "tsc -p main",
    "next:dev": "next dev",
    "next:start": "next start",
    "next:lint": "next lint",
    "electron:dev": "npm run build:main && wait-on tcp:3000 && electron .",
    "electron": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "@react-bulk/core": "^0.7.3",
    "@react-bulk/web": "^0.7.3",
    "autoprefixer": "^10.4.17",
    "caret-pos": "^2.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron-serve": "^1.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "js-cookie": "^3.0.5",
    "mysql2": "^3.9.1",
    "next": "13.5.6",
    "oracledb": "^6.3.0",
    "pg": "^8.11.3",
    "rangy": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-resizable-panels": "^0.0.57",
    "react-state-hooks": "^0.1.9",
    "sql-highlight": "^4.4.0",
    "swr": "^2.2.4",
    "uuid": "^9.0.1",
    "wait-on": "^7.2.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/js-cookie": "^3.0.6",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.11.17",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "electron": "^26.6.9",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "build": {
    "appId": "com.caioedut.dbconn",
    "productName": "DbConn",
    "copyright": "Copyright Â© CaioEduT",
    "win": {
      "icon": "./resources/icon.png",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": true
    },
    "mac": {
      "icon": "./resources/icon.png",
      "category": "your.app.category.type",
      "target": [
        "dmg"
      ]
    },
    "dmg": {
      "title": "Mac Installer"
    },
    "linux": {
      "icon": "./resources/icon.png"
    }
  }
}
